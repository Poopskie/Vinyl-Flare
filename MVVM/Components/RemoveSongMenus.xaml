<UserControl x:Class="Vinyl_Flare.MVVM.Components.RemoveSongMenus"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Vinyl_Flare.MVVM.Components"
             xmlns:commands ="clr-namespace:Vinyl_Flare.MVVM.Commands"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type= commands:RemoveSongCommand}"
             d:DesignHeight="450" d:DesignWidth="800"
             Background="AliceBlue">
   
    <Grid>
        <ContentControl Content="{Binding SongArray[0].SongName}"
                            />
    </Grid>
</UserControl>
